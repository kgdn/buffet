  "scripts": {
    "dev": "VITE_COMMIT_HASH=`git rev-parse --short HEAD` VITE_VERSION=`node -p \"require('./package.json').version\"` VITE_COMMIT_DATE=`git log -1 --format=%cd --date=iso --pretty=format:%cd` DEV=true vite",
    "build": "tsc && VITE_COMMIT_HASH=`git rev-parse --short HEAD` VITE_VERSION=`node -p \"require('./package.json').version\"` VITE_COMMIT_DATE=`git log -1 --format=%cd --date=iso --pretty=format:%cd` vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "VITE_COMMIT_HASH=`git rev-parse --short HEAD` VITE_VERSION=`node -p \"require('./package.json').version\"` VITE_COMMIT_DATE=`git log -1 --format=%cd --date=iso --pretty=format:%cd` vite preview"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
